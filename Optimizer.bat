@echo off
REM Check if the script is running as administrator
NET FILE 1>NUL 2>NUL
if '%errorlevel%' == '0' (goto :start) else (goto :runasadmin)

:start
REM The script starts here
title System Tweaks Menu

REM Set text color to cyan
color 0B

:optimizer
rem Making directories...
mkdir %SYSTEMDRIVE%\optimizer >nul 2>&1
mkdir %SYSTEMDRIVE%\optimizer\Resources >nul 2>&1
mkdir %SYSTEMDRIVE%\optimizer\optimizerRevert >nul 2>&1
mkdir %SYSTEMDRIVE%\optimizer\Drivers >nul 2>&1
mkdir %SYSTEMDRIVE%\optimizer\Renders >nul 2>&1
rem Directories created.

rem Add registry entry to run the script as administrator
reg add "HKCU\Software\Microsoft\Windows NT\CurrentVersion\AppCompatFlags\Layers" /v "%~0" /t REG_SZ /d "RUNASADMIN" /f >nul 2>&1

rem Show detailed BSOD information
reg add "HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\CrashControl" /v "DisplayParameters" /t REG_DWORD /d 1 /f >nul 2>&1

rem Add registry entry under HKCU\Console
reg add "HKCU\Console" /v "VirtualTerminalLevel" /t REG_DWORD /d 1 /f >nul 2>&1

:license
cls
echo User License Agreement
echo ----------------------

REM Display the license agreement
type "C:\Optimizer\license.txt"

echo.
set /p agree=Do you agree to the terms and conditions? (yes/no): 

REM Check user input
if /i "%agree%"=="yes" (
    echo You have agreed to the terms and conditions.
    REM Continue with your script's operations here...
    goto menu
) else (
    echo You have not agreed to the terms and conditions. Exiting...
    exit /b 1
)

:menu
cls
echo System Tweaks Menu
echo ------------------
echo 0. About
echo 1. General Tweaks
echo 2. Network Tweaks
echo 3. Memory Tweaks
echo 4. CPU Tweaks
echo 5. Game Tweaks
echo 6. Mouse Tweaks
echo 7. Keyboard Tweaks
echo 8. MSI Mode
echo 9. Backup Registry and Create Restore Point
echo 10. Restore Registry and Windows Restore Point
echo 11. Windows Settings
echo 12. Exit

set "choice="
set /p choice=Enter your choice: 

rem Check if the input is empty
if "%choice%"=="" (
    set "choice="
    goto menu
)

rem Process the user's choice
if "%choice%"=="0" goto about
if "%choice%"=="1" goto general
if "%choice%"=="2" goto network_menu
if "%choice%"=="3" goto memory
if "%choice%"=="4" goto cpu
if "%choice%"=="5" goto game
if "%choice%"=="6" goto mouse
if "%choice%"=="7" goto kbm
if "%choice%"=="8" goto msi
if "%choice%"=="9" goto backup_registry_and_restore_point
if "%choice%"=="10" goto restore_registry_and_restore_point
if "%choice%"=="11" goto windows_settings
if "%choice%"=="12" goto end

rem If the input is not a valid choice, display an error message and prompt again
echo Invalid choice. Please enter a valid choice.
timeout /t 2 >nul
goto menu

